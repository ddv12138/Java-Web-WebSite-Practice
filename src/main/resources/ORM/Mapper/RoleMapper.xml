<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ORM.Mapper.RoleMapper">
    <select id="listRole" resultType="ORM.POJO.Role">
        select id, name, "desc", "lock"
        From roletable
        order by id
    </select>
</mapper>